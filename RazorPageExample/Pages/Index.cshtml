@page
@model IndexModel
@using RazorPageExample.BussinesLogic.ViewModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>


  
        <h4>Customers</h4>
        <hr />
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>PositionId</th>
                <th>PositionDescription</th>
            </tr>
            @foreach (PositionsViewModel positionsViewModel in Model.positions )
            {
                <tr>
                    <td>@positionsViewModel.PositionId</td>
                    <td>@positionsViewModel.PositionDescription</td>
                </tr>
            }
        </table>
    
    
</div>
